  "require": {
    "symfony/process": "^5.0 || ^6.0 || ^7.0"
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit",
      "./vendor/bin/phpcs -p -n"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7 || ^8 || ^9",
    "squizlabs/php_codesniffer": "^3.3",
    "phpstan/phpstan": "^1.0"
  }
}
