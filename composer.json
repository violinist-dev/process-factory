{
  "name": "violinist-dev/process-factory",
  "description": "A factory for symfony/process processes",
  "license": "MIT",
  "authors": [
    {
      "name": "Eirik S. Morland",
      "email": "eirik@morland.no"
    }
  ],
  "autoload": {
    "psr-4": {
      "Violinist\\ProcessFactory\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Violinist\\ProcessFactory\\Tests\\": "tests"
    }
